{
  "name": "prestigos-sdk.www",
  "version": "0.0.1",
  "description": "Documentation website for Prestigos-SDK",
  "dependencies": {},
  "devDependencies": {
    "browserify": "13.0.1",
    "browserify-shim": "3.8.12",
    "uglify": "0.1.5"
  },
  "browser": {
    "jquery": "./bower_components/jquery/dist/jquery.js",
    "bootstrap": "./bower_components/bootstrap/dist/js/bootstrap.js",
    "modernizr": "./bower_components/modernizr/modernizr.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "jquery": "$",
    "bootstrap": {
      "depends": [
        "jquery:$"
      ]
    },
    "modernizr": "Modernizr"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/prestigos/website-prestigos-sdk.git"
  },
  "scripts": {
    "dev": "node_modules/browserify/bin/cmd.js --no-builtins js/main.js > public/js/main.min.js",
    "build": "node_modules/browserify/bin/cmd.js --no-builtins js/main.js | node_modules/uglify-js/bin/uglifyjs -o public/js/main.min.js"
  }
}
